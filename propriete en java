([])==optionnel

private StringProperty toto=new SimpleStringProperty()

//getter
public String getToto(){  //nom super important bon->get(Nom)=/=get(nom)
    return toto.get();
}


//setter
public void setToto(String val){ //idem
    toto.set(val);
}

//accesseur
public ([ReadOnly])StringProperty totoProperty(){//idem
    return toto;
}


